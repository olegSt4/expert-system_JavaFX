<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.TextField?>
<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="lab3.controllers.MainWindowController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <TextField fx:id="skillsLine" layoutX="174.0" layoutY="161.0" prefHeight="26.0" prefWidth="376.0" />
            <Label layoutX="46.0" layoutY="164.0" text="Write your skills: ">
               <font>
                  <Font name="System Bold Italic" size="14.0" />
               </font>
            </Label>
            <Button onAction="#acceptSkills" layoutX="458.0" layoutY="200.0" mnemonicParsing="false" text="Accept skills" />
      </children>
    </AnchorPane>
  </children>
</VBox>
